<h1>List of Users Created by <%= current_user.name %></h1>

<ul>
<% @users.reject{|u| u.admin?}.each do |user| %>
  <li><%= user.name %>&nbsp;<span class="hint">added <%= human_time(user.created_at) %></span>&nbsp;<%= link_to "edit", edit_user_path(user) %>&nbsp;
<% end %>
</ul>
<%= link_to "New user", new_user_url %>