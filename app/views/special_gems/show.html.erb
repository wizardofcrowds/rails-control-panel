<h1>Gem Installation Status</h1>

<p>
  <%= label_tag 'Request Time' %><br/>
  <%= human_time(@special_gem.created_at) %>
</p>

<p>
  <%= label_tag 'name' %><br/>
  <%= @special_gem.name %>
</p>

<p>
  <%= label_tag 'version' %><br/>
  <%= @special_gem.version %>
</p>

<p>
  <%= label_tag 'status' %><br/>
  <%= @special_gem.status %>
</p>

<h2>Installation Results</h2>
<p>
  <%= label_tag 'Installation command exit status' %><br/>
<pre>
  <%= @special_gem.result_status %>
</pre>
</p>

<p>
  <%= label_tag 'Stdout' %><br/>
<pre>
  <%= @special_gem.result_stdout %>
</pre>
</p>

<p>
  <%= label_tag 'Error Messages' %><br/>
<pre>
  <%= @special_gem.result_stderr %>
</pre>
</p>
<br/>
<%= link_to "List of Gems", special_gems_url %>&nbsp;|&nbsp;<%= link_to "Back to console", user_path(current_user) %>

